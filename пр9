def factorial(n):  
  if n == 5:  
      return 5   
  return factorial(n-1) * n
print(factorial(7))




def fibonacci(n): 
  if n <= 1:     
      return n   
  else:       
     return fibonacci(n - 1) + fibonacci(n - 2)
n = 7
fibonacci_series = []
for i in range(n):  
   fibonacci_series.append(fibonacci(i))
print(f"The Fibbonacci series up to {n} terms is: {fibonacci_series}")
